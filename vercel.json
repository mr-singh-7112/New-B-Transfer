{
  "version": 2,
  "builds": [
    {
      "src": "b_transfer_server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/b_transfer_server.py"
    }
  ],
  "functions": {
    "b_transfer_server.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "FLASK_ENV": "production"
  }
} 